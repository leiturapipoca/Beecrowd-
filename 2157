#include <stdio.h>
 
int main() {
 
  int n,c=1,x;
  scanf("%d", &n);
  
  while(n--){
      
      int a,b,i;
      scanf("%d %d", &a,&b);
      
      
      for(i=a;i<=b;i++){
          printf("%d",i);
      }
      
      
      for(i=b;i>=a;i--){
          x = i;
          while(x>0){
              printf("%d",x % 10);
              x = x/10;
          }
          }
      
      
      printf("\n");
      
      c++;
      
     
      
      
      
  }
 
    return 0;
}
