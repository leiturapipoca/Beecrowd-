#include <stdio.h>
 
int main() {
 
    int i, n,vet[5],foi=0;
    scanf("%d",&n);
    scanf("%d",&vet[0]);
    scanf("%d",&vet[1]);
    scanf("%d",&vet[2]);
    scanf("%d",&vet[3]);
    scanf("%d",&vet[4]);
    
 for(i=0;i<5;i++){
     if(vet[i]== n){
         foi++;
     }
 }
 printf("%d\n",foi);
    return 0;
}
