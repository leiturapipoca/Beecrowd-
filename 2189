#include <stdio.h>
 
int main() {
 
 int cont=0;
 while(1){
     int n;
     scanf("%d",&n);
 if(n==0){
     break;
 }
     

int i,sorte;
int vet[n + 1];
 
 for(i=1;i<=n;i++){
     scanf("%d",&vet[i]);
     if(vet[i]==i){
         sorte = i;
     }
 }
 cont++;
 printf("Teste %d\n", cont);
 printf("%d\n",sorte);
 printf("\n");
 }
    return 0;
}
