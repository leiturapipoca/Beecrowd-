#include<bits/stdc++.h>
#include<vector>
#include <list>
#include<stack>
#include<algorithm>
#include <iostream>
#include <string>
#include <unordered_map>
#include <queue>
typedef long long ll;

using namespace std;

int main(){
    std::ios::sync_with_stdio(false);
    cin.tie(NULL);
while(1){
int x,y;
    cin>>x>>y;
    
    if(x==y){
        return 0;
    }

    if(x>y){
        cout<<"Decrescente\n";
    } else{
        cout<<"Crescente\n";
    }
}
    

    return 0;
}
